{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "supabase.ts", "**/*.ts", "store.js"],
  "exclude": ["src/**/__tests__/*"],
  "files": ["src/main.ts","store.js"],
  "compilerOptions": {
    "allowJs": true,
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
